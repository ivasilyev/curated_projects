@article{my2012,
    abstract = {В работе исследованы закономерности роста и пигментообразования у биотехнологически важных актинобактерий Agreia, пигмент которых предложен нами к использованию в качестве топливного маркера. Подобраны условия культивирования продуцента, установлен оптимум продукции биомаркера: максимальный выход целевого продукта наблюдается на 72-й ч роста на коринебактериальном агаре. Рост бактерий на жидких средах не индуцировал пигментообразование, хотя аэрация способствовала накоплению биомассы. Освещение не влияло на образование пигмента, следовательно, технология получения биомаркера может быть удешевлена за счет выращивания бактерий-продуцентов в темноте. The paper studies growth patterns of and pigment production in biotechnologically significant actinobacteria Agreia. We have earlier suggested using their pigment as a biomarker for fuels. The conditions for the producer incubation have been selected, and the optimum of the biomarker production has been determined. Maximum biosynthesis of the desired product has been observed at the 72th hour of growth on the corynebacterial agar. The culturing in liquid media has not induced pigment formation although aeration has promoted the accumulation of biomass. Lightening has not influenced pigment formation; therefore, the cost of the biomarker production can be significantly reduced through incubation in the dark.},
    author = {Глазунова Евгения Геннадьевна, Яруллина Дина Рашидовна, Васильев Илья Юрьевич, Гаврилова Мария Николаевна, Ильинская Ольга Николаевна},
    udk = {579.22+579.083.13},
    issn = {2542-064X},
    eissn = {2500-218X7},
    journal = {Ученые Записки Казанского Университета. Серия: Естественные Науки},
    keywords = {актинобактерии agreia, actinobacteria agreia, пигмент, pigment, спектр поглощения, absorption spectrum, динамика роста, growth dynamics, аэрация, aeration, освещение, состав питательной среды, medium composition, lightening},
    number = {2},
    pages = {77--84},
    elibraryid = {18042916},
    title = {{Биосинтез пигментов у актинобактерий Agreia в зависимости от условий культивирования}},
    url = {https://www.elibrary.ru/item.asp?id=18042916},
    volume = {154},
    year = {2012}
}
